   "source": [
    "#fractals in a square with minimum changes\n",
    "a = np.array([-1,1])\n",
    "b = np.array([1,1])\n",
    "c = np.array([1,-1])\n",
    "d = np.array([-1,-1])\n",
    "o = np.array([0,0])\n",
    "level = 4\n",
    "gasket(o,a,b, level,'g')\n",
    "gasket(o,b,c, level,'b')\n",
    "gasket(o,c,d, level,'o')\n",
    "gasket(o,d,a, level,'r')\n",
    "\n",
    "plt.title(\"Lv.4\")\n",
    "plt.axis('equal')"
   ]

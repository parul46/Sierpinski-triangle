    "get_ipython().magic(u'matplotlib inline')\n",
    "import numpy as np\n",
    "import matplotlib.pylab as plt\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [],
   "source": [
    "#Generating a Sierpinski triangle\n",
    "def gasket(pa, pb, pc, level,color):\n",
    "    if level == 0: \n",
    "        plt.fill([pa[0], pb[0], pc[0]], [pa[1], pb[1], pc[1]], color) \n",
    "       \n",
    "    else:\n",
    "        gasket(pa, (pa + pb) / 2., (pa + pc) / 2., level - 1,color) \n",
    "        gasket(pb, (pb + pa) / 2., (pb + pc) / 2., level - 1,color) \n",
    "        gasket(pc, (pc + pa) / 2., (pc + pb) / 2., level - 1,color)\n",
    "        \n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},

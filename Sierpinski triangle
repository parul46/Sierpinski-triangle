    "a = np.array([-1,1])\n",
    "b = np.array([1,1])\n",
    "o = np.array([0,0])\n",
    "level = 4\n",
    "gasket(o,a,b, level,'g')\n",
    "\n",
    "\n",
    "plt.title(\"Lv.4\")\n",
    "plt.axis('equal')"
